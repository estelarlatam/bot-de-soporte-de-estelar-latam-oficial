{
  "name": "voicetwine",
  "version": "1.4.0",
  "description": "Voice channel manager for Discord",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node -v && node dist/app.js",
    "test": "npm run build && npm run start",
    "clean": "rm -rf dist/",
    "lint": "eslint src/**/*.ts",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:status": "sequelize-cli db:migrate:status"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Twijn/VoiceTwine.git"
  },
  "keywords": [
    "discord-bot",
    "discord",
    "bot"
  ],
  "author": "Twijn",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/Twijn/VoiceTwine/issues"
  },
  "homepage": "https://github.com/Twijn/VoiceTwine#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.22.0",
    "pino-pretty": "^13.0.0",
    "sequelize-cli": "^6.6.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  },
  "dependencies": {
    "discord-api-types": "^0.38.15",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "mariadb": "^3.4.2",
    "mysql2": "^3.14.1",
    "pino": "^9.6.0",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7"
  }
